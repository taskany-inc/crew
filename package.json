    "@tanstack/react-query-devtools": "4.33.0",
    "@taskany/bricks": "3.1.1",
    "@taskany/colors": "1.1.0",
    "@taskany/icons": "1.1.2",
    "@trpc/client": "10.36.0",
    "@trpc/next": "10.36.0",
    "@trpc/react-query": "10.36.0",
    "@trpc/server": "10.36.0",
    "bcryptjs": "2.4.3",
    "easy-typed-intl": "1.0.3",
    "next": "13.4.9",
    "next-auth": "4.22.4",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.4",
    "styled-components": "5.3.11",
    "superjson": "1.13.1",
    "throttle-debounce": "5.0.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@next/bundle-analyzer": "13.4.19",
    "@svgr/webpack": "8.0.1",
    "@types/bcryptjs": "2.4.2",
