{
  "name": "@taskany/crew",
  "version": "0.53.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && next dev",
    "dev:db:init": "npx prisma migrate dev --preview-feature && npx prisma db seed",
    "dev:db:run": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker-compose down",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "build": "npm run build:css && next build",
    "start": "next start",
    "lint": "next lint && eslint --ext .js,.ts,.tsx .",
    "generate:i18n": "generate-i18n --func=tr --langs=en,ru --path='./src/**/*' --getLang='./src/utils/getLang'",
    "test": "node --import tsx --test ./src/**/*.test.*",
    "test:watch": "node --import tsx --test --watch ./src/**/*.test.*",
    "prepare": "husky install",
    "type-check": "npx tsc",
    "build:css": "npx postcss src/styles/dark.css src/styles/light.css -d public/theme"
  },
  "prisma": {
    "seed": "tsx -r dotenv/config prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@next-auth/prisma-adapter": "1.0.7",
    "@sentry/nextjs": "7.99.0",
    "@swc/core": "1.4.11",
    "@tanstack/react-query": "4.35.7",
    "@tanstack/react-query-devtools": "4.35.7",
    "@taskany/bricks": "5.29.1",
    "@taskany/colors": "1.11.0",
    "@taskany/icons": "2.0.5",
    "@trpc/client": "10.36.0",
    "@trpc/next": "10.36.0",
    "@trpc/react-query": "10.36.0",
    "@trpc/server": "10.36.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.5.1",
    "easy-typed-intl": "1.0.3",
    "next": "13.4.9",
    "next-auth": "4.24.7",
    "next-themes": "0.2.1",
    "nodemailer": "6.9.13",
    "postcss-cli": "11.0.0",
    "postcss-import": "16.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.1",
    "react-hot-toast": "2.4.1",
    "styled-components": "5.3.11",
    "superjson": "1.13.1",
    "swagger-ui-react": "5.12.3",
    "throttle-debounce": "5.0.0",
    "trpc-openapi": "1.2.0",
    "use-debounce": "10.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "18.6.0",
    "@next/bundle-analyzer": "14.1.0",
    "@svgr/webpack": "8.1.0",
    "@taskany/eslint-plugin-rules": "0.1.3",
    "@types/bcryptjs": "2.4.5",
    "@types/node": "20.11.30",
    "@types/nodemailer": "6.4.14",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.25",
    "@types/styled-components": "5.1.26",
    "@types/swagger-ui-react": "4.18.3",
    "@types/throttle-debounce": "5.0.2",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@welldone-software/why-did-you-render": "7.0.1",
    "dotenv": "16.3.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "2.8.8",
    "prisma": "5.7.0",
    "tsx": "4.7.0",
    "typescript": "5.4.5",
    "typescript-plugin-css-modules": "5.1.0",
    "webpack": "5.90.1"
  }
}
